{
  "name": "eoneopay/framework",
  "description": "The EoneoPay Framework",
  "keywords": [
    "eoneopay",
    "framework"
  ],
  "license": "BSD-3-Clause",
  "type": "project",
  "require": {
    "php": ">=7.2",
    "eoneopay/apiformats": "dev-master",
    "eoneopay/externals": "^1.0",
    "eoneopay/utils": "^0.2",
    "ext-json": "*",
    "laravel/lumen-framework": "^5.5",
    "laravel-doctrine/orm": "^1.4",
    "vlucas/phpdotenv": "^3.0"
  },
  "require-dev": {
    "eoneopay/standards": "dev-master",
    "phpmd/phpmd": "^2.6",
    "phpstan/phpstan": "^0.11",
    "phpunit/phpunit": "^7.0",
    "roave/security-advisories": "dev-master",
    "sebastian/phpcpd": "^4.0",
    "squizlabs/php_codesniffer": "3.*"
  },
  "autoload": {
    "psr-4": {
      "EoneoPay\\Framework\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\EoneoPay\\Framework\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true
  }
}
