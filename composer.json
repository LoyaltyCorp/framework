{
  "name": "eonx-com/framework",
  "description": "The EonX Framework",
  "keywords": [
    "eoneopay",
    "framework"
  ],
  "license": "BSD-3-Clause",
  "type": "project",
  "require": {
    "php": ">=7.2",
    "eonx-com/apiformats": "^1.0",
    "eonx-com/externals": "^1.0|^2.0",
    "eonx-com/utils": "^1.0",
    "ext-json": "*",
    "laravel/lumen-framework": "^6.0",
    "laravel-doctrine/orm": "^1.5.4",
    "vlucas/phpdotenv": "^3.0"
  },
  "require-dev": {
    "eonx-com/standards": "^0.2",
    "friendsofphp/php-cs-fixer": "^2.15",
    "phpmd/phpmd": "^2.6",
    "phpstan/phpstan": "^0.11",
    "phpstan/phpstan-phpunit": "^0.11",
    "phpstan/phpstan-strict-rules": "^0.11",
    "phpunit/phpunit": "^8.0",
    "roave/security-advisories": "dev-master",
    "sebastian/phpcpd": "^4.0",
    "squizlabs/php_codesniffer": "3.*"
  },
  "replace": {
    "eoneopay/framework": "^0.2"
  },
  "autoload": {
    "psr-4": {
      "EoneoPay\\Framework\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2.x-dev"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\EoneoPay\\Framework\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true
  }
}
